{
  "name": "near-directory",
  "version": "0.0.3",
  "type": "module",
  "packageManager": "bun@1.3.8",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "serve -s dist -l ${PORT:-3000}",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "test:e2e": "npx playwright test -c playwright-tests/playwright.config.ts",
    "test:e2e:ui": "npx playwright test -c playwright-tests/playwright.config.ts --ui",
    "test:e2e:headed": "npx playwright test -c playwright-tests/playwright.config.ts --headed",
    "test:e2e:debug": "npx playwright test -c playwright-tests/playwright.config.ts --debug",
    "test:e2e:report": "npx playwright show-report playwright-tests/playwright-report"
  },
  "dependencies": {
    "@hot-labs/near-connect": "latest",
    "@near-kit/react": "latest",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.157.18",
    "borsh": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "near-kit": "latest",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph-3d": "^1.25.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.12.0",
    "serve": "^14.2.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "@playwright/test": "^1.50.0",
    "vite": "^6.0.1",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
